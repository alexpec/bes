FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/Air_rotated";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    Air
    {
        type            zeroGradient;
    }
    Air_top
    {
        type            zeroGradient;
    }
    AirBottomLine_rotated
    {
        type            zeroGradient;
    }
    Air_rotated_to_Calorimeter_rotated
    {
        type            calculated;
        value           uniform 0;
    }
    Air_rotated_to_Barrel_rotated
    {
        type            calculated;
        value           uniform 0;
    }
}



# Demonstrate Dakota script interface to Python analysis (Rosenbrock)

environment
	graphics
	tabular_data
		tabular_data_file = 'rosen_opt_patternsearch.dat'

method,						
	optpp_newton
	  max_iterations = 100			
	  convergence_tolerance = 1e-4
	
	#max_iterations = 100
	#convergence_tolerance = 1e-4
	#conmin_frcg
	
	#max_iterations = 1000
	#max_function_evaluations = 2000
	#coliny_pattern_search
	#	solution_accuracy = 1e-4
	#	initial_delta = 0.5
	#	threshold_delta = 1e-4
	#	exploratory_moves basic_pattern
	#	contraction_factor = 0.75
	
variables,					
	continuous_design = 2			
	  cdv_initial_point  -5.2  5.0		
	  cdv_lower_bounds   -10.0 -10.0		
	  cdv_upper_bounds    10.0  10.0		
	  cdv_descriptor      'x1' 'x2'

interface,							
	fork							
      analysis_drivers = 'Rosenbrock.py'
	parameters_file = 'params.in'
	results_file = 'results.out'
	
	#file_tag
	#file_save

responses,	
	num_objective_functions = 1	
	numerical_gradients
	numerical_hessians
	#analytic_gradients		
	#analytic_hessians
